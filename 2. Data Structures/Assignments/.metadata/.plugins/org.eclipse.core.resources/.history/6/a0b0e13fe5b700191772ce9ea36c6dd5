#include <stdio.h>
#include <stdlib.h>

typedef struct node{
	long int parents;
	unsigned long int nodes;
}node;

int main(){
	unsigned long int n;

	scanf("%lu", &n);
	fflush(stdin);

	node *ptrNodes = (node*) malloc(n * sizeof(node));
	for(unsigned long int i = 0; i < n; i++){
		scanf("%ld", (*node).parents);
	}
	return 0;
}
