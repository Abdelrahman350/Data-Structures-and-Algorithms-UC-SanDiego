#include <iostream>
#include <vector>
#include <algorithm>
using namespace std;

typedef struct node{
	long int parents;
	unsigned long int nodes;
}node;

int main(){
	unsigned long int n;
	unsigned int height = 0;

	cin >> n;

	vector <node> nodes;
	nodes.reserve(n);
	for(unsigned long int i = 0; i < n; i++){
		cin >> (*ptrNodes).parents;
		(*ptrNodes).nodes = i;
	}
	height = treeHeight(ptrNodes);
	cout << height;
	return 0;
}

unsigned int treeHeight()
